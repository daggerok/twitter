{
  "name": "query-tweet-react-web",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "start": "npm-run-all -p webpack-dev-server open-url",
    "webpack-dev-server": "webpack-dev-server --env development -w -d --config=./config/webpack.config.babel.js",
    "open-url": "npm-run-all sleep url",
    "sleep": "sleep 3",
    "url": "open-url localhost:8000/react/",

    "test": "eslint ./src ./config -c ./config/.eslintrc.js --ext js './src/**/*.js' './config/**/*.js'",

    "precompile": "rimraf -rf ../../src/main/resources/public/react",
    "compile": "webpack --config=./config/webpack.config.babel.js",

    "build": "npm run compile -- --env production",

    "serve": "npm-run-all build start-server",
    "start-server": "browser-sync start -c ./config/bs-config.js"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "jquery": "3.1.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-addons-pure-render-mixin": "15.4.2",
    "react-router": "3.0.2",
    "react-router-scroll": "0.4.1",
    "react-http-request": "1.0.3",
    "rx-connect": "0.6.1",
    "rx": "4.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.6",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.0.0",
