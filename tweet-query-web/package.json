{
  "name": "tweet-query-web",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.js --env development",
    "prewebpack": "rimraf -rf ../tweet-query/src/main/resources/static",
    "webpack": "webpack --config ./config/webpack.config.js",

    "lint": "tslint -c ./config/tslint.json 'src/**/*.ts' 'e2e/**/*.ts'",

    "unit": "karma start ./config/karma.conf.js",

    "e2e": "concurrently 'npm run protractor' 'npm run server' --kill-others --success first -p '[{name}]' -n 'E2E,HTTP' -c 'green,cyan' # colors: black, red, green, yellow, blue, magenta, cyan, white, gray",
    "pree2e": "npm run webpack",
    "protractor": "protractor ./config/protractor.conf.js",
    "preprotractor": "npm run webdriver-manager-update",
    "prewebdriver-manager-update": "sleep 3",
    "webdriver-manager-update": "webdriver-manager update  --standalone false --gecko false --quiet",

    "start-server": "browser-sync start -c ./config/lite-server.config.js",
    "server": "npm run start-server -- --no-notify --no-open --no-online --no-inject-changes --no-reload-on-restart",

    "preserve": "npm run build",
    "build": "npm run webpack -- -p --env production",
    "serve": "lite-server -c ./config/lite-server.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/router": "3.4.8",
    "core-js": "2.4.1",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.0",
    "@angular/compiler-cli": "2.4.8",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.5",
    "autoprefixer": "6.7.5",
    "codelyzer": "3.0.0-beta.0",
    "concurrently": "3.3.0",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.26.2",
    "cssnano": "3.10.0",
    "exports-loader": "0.6.4",
    "file-loader": "0.10.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "2.2.3",
    "lite-server": "2.2.2",
    "postcss-loader": "1.3.2",
    "postcss-url": "5.1.2",
    "protractor": "5.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.2",
    "script-loader": "0.7.0",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.2",
    "stylus-loader": "2.5.0",
    "ts-node": "2.1.0",
    "tslint": "4.5.0",
    "typescript": "2.2.1",
    "url-loader": "0.5.8",
    "webpack-dev-server": "2.4.1"
  }
}
